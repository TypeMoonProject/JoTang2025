    "import sympy\n",
    "x,y=sympy.symbols('x y')\n",
    "f=x*y+x+y\n",
    "fx=sympy.diff(f,x)\n",
    "fy=sympy.diff(f,y)\n",
    "a=(fx.subs({x:1,y:1}),fy.subs({x:1,y:1}))\n",
    "print(a)"
   ]
  }
